{
	"name": "obsidian-knowledge-manager",
	"version": "1.0.0",
	"description": "A plugin for managing and organizing knowledge in Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && node copy-templates.mjs",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"setup": "git submodule update --init --recursive && npm install",
		"setup:dev": "npm run setup && cd src/libs/obsidian-utils && npm install"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@langchain/community": "^0.3.44",
		"@langchain/core": "^0.3.40",
		"@langchain/deepseek": "^0.0.1",
		"@langchain/google-genai": "^0.1.8",
		"@langchain/groq": "^0.1.3",
		"@langchain/ollama": "^0.2.0",
		"@langchain/openai": "^0.5.11",
		"@orama/orama": "^2.1.1",
		"@orama/plugin-data-persistence": "^2.1.1",
		"double-metaphone": "^2.0.1",
		"fastest-levenshtein": "^1.0.16",
		"langchain": "^0.3.27",
		"talisman": "^1.1.4",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@babel/core": "^7.26.0",
		"@babel/preset-env": "^7.26.0",
		"@types/jest": "^29.5.11",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"assemblyai": "^4.9.0",
		"babel-jest": "^29.7.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^29.7.0",
		"obsidian": "^1.8.7",
		"ts-jest": "^29.1.1",
		"tslib": "2.4.0",
		"typescript": "^5.4.5"
	}
}
